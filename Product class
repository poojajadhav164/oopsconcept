package com.tka.jan1;

public class Product1 {
	

	    private int pid;
	    private String pname;
	    private double pprice;
	    private String pcategories;
	    private String pbrand;
	    private int pquantity;

	    public Product1(int pid, String pname, double pprice, String pcategories,String pbrand, int pquantity) {
	        this.pid = pid;
	        this.pname = pname;
	        this.pprice = pprice;
	        this.pcategories = pcategories;
	        this.pbrand = pbrand;
	        this.pquantity = pquantity;
	    }

	    // Getters and Setters
	    public int getPid() {
	        return pid;
	    }

	    public void setPid(int pid) {
	        this.pid = pid;
	    }

	    public String getPname() {
	        return pname;
	    }

	    public void setPname(String pname) {
	        this.pname = pname;
	    }

	    public double getPprice() {
	        return pprice;
	    }

	    public void setPprice(double pprice) {
	        this.pprice = pprice;
	    }

	    public String getPcategories() {
	        return pcategories;
	    }

	    public void setPcategories(String pcategories) {
	        this.pcategories = pcategories;
	    }

	    public String getPbrand() {
	        return pbrand;
	    }

	    public void setPbrand(String pbrand) {
	        this.pbrand = pbrand;
	    }

	    public int getPquantity() {
	        return pquantity;
	    }

	    public void setPquantity(int pquantity) {
	        this.pquantity = pquantity;
	    }

	    // toString()
	    @Override
	    public String toString() {
	        return pid + " | " + pname + " | " + pprice + " | " +
	               pcategories + " | " + pbrand + " | " + pquantity;
	    }
	}
	    
	
